**      STOBLK
*
*	CALCULATE BLOCK ADDRESS FROM SECTOR IN HL. WRITE TO
*	BARH/BARL.
*
*	BAR = SECNO<<2
*
*	DESTROYS
*	    A

STOBLK	EQU	*
	PUSH	H
*	XRA	A			* CLEAR CARRY ... XXX DOUBLE IS ONLY SHL-1
*	MOV	A,L			* ROTATE L
*	RAL
*	MOV	L,A
*	MOV	A,H			* ROTATE H, INHERIT CARRY
*	RAL
*	MOV	H,A
	XRA	A			* CLEAR CARRY
	MOV	A,L			* ROTATE L
	RAL
	STA	BARL
	MOV	A,H			* ROTATE H, INHERIT CARRY
	RAL
	STA	BARH
	POP	H
	RET
